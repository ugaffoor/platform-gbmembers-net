  "customHeadContent": "<style>\n  div[data-element-type='content'] a {\n      color: blue;\n  }    \n  .errorDetails {\n      background-color: red;\n      border-radius: 5px;\n      margin: 5px;\n  }  \n  \n</style>  \n<script>\n  function GetURLParameter(sParam){\n    arr=window.location.hash.split(sParam+\"=\");\n    if (arr.length>0){\n      return arr[1];\n    }\n    return null;\n  }  \n  function isPrimaryBiller(){\n    var parentMember=K('field[Billing Parent Member]').value();\n    var familyMembers=K('field[Billing Family Members]').value();\n    debugger;\n   \tif (K('field[Members]').value()===parentMember && familyMembers!==null && familyMembers!==undefined){\n      var dependants=JSON.parse(familyMembers);\n      if (dependants.length>1) {\n\t      return true;\n      }\n    }\n    return false;\n  }\n  \n</script>\n",
