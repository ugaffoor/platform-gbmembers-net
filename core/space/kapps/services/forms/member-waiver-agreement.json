          "code": "bundle.waiverInfo=\"\";\nbundle.GetURLParameter = function(sParam){\n    arr=window.location.hash.split(sParam+\"=\");\n    if (arr.length>0){\n      return arr[1];\n    }\n    return null;\n  }  \n  \nbundle.agreeTerms = function() {\n    K('field[Agreement]').value(\"Agreed\");\n    K('form').submitPage();\n  }\nbundle.noAgreeTerms = function() {\n    K('field[Agreement]').value(\"NOT Agreed\");\n    K('form').submitPage();\n  }\n\n\nK('bridgedResource[Members]').load({\n  attributes: ['Member ID', 'First Name', 'Last Name', 'Covid19 Waiver'],\n  success: function(bridgedData) {\n debugger;\n    var options=[];\n    $.each( bridgedData, function( key, value ) {\n\t      options[options.length]={'label':value['First Name']+\" \"+value['Last Name'],'value':value['id']};\n    });\n\t\tK('field[Members]').options(options);\n    \n    var label=null;\n\t\tK('field[Members]').options(options);\n    if (bundle.GetURLParameter(\"id\")!==null && bundle.GetURLParameter(\"id\")!==undefined) {\n    \tK('field[Members]').value(bundle.GetURLParameter(\"id\"));\n      $(K('field[Members]').options()).each(function(index) {\n       if (K('field[Members]').options()[index].value===bundle.GetURLParameter(\"id\")) label=K('field[Members]').options()[index].label;\n      });      \n    } else {\n\t\t\tif (!K(\"identity\").teams.includes(\"Role::Program Managers\")) {      \n        K('bridgedResource[Get Member by Username]').load({\n          attributes: ['Member ID', 'First Name', 'Last Name', 'Covid19 Waiver'],\n          success: function(bridgedData) {\n\t         \tdebugger;\n            K('field[Members]').value(bridgedData['id']);\n\t\t\t    \tK('section[Select Member]').hide();      \n          }\n        });\n      }\n    }\n    \n    bundle.config.widgets.selectMenu({\n      element: K(\"form\").find(\"#selectMemberMenu\")[0],\n      options: options,\n      onChange: function(option) {\n        K('field[Members]').value(option.value);\n        setTimeout(function(){\n          $(K(\"form\").find(\"#selectMemberMenu .css-1492t68\")).html(option.label);\n        },100);\n      },\n      value: K('field[Members]').value(),\n    });  \n    \n    if (label!==null){\n    \tK('section[Select Member]').hide();      \n    }    \n  }\n});\n",
