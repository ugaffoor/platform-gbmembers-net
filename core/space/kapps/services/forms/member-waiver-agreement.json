                  "action": "Custom",
                  "code": "K('bridgedResource[Get Member]').load({\n  attributes: ['Member ID', 'First Name', 'Last Name'],\n  values: {'ID' : 'Allen'},\n  success: function(bridgedData) {\n    K('field[Member ID]').value(bridgedData['Member ID']);\n    K('field[Student First Name]').value(bridgedData['First Name']);\n    K('field[Student Last Name]').value(bridgedData['Last Name']);\ndebugger;\n    info=bundle.waiverInfo;\n    \n    var schoolName=K('space').attributes('School Name');\n    if (schoolName.startsWith(\"GB \")){\n      schoolName=schoolName.replace(\"GB \",\"\"); \n    }\n    if (schoolName.startsWith(\"Gracie Barra \")){\n      schoolName=schoolName.replace(\"Gracie Barra \",\"\"); \n    }\n    var schoolEmail=K('space').attributes('School Email');\n    var acn=K('space').attributes('ACN');\n    var schoolLegalName=K('space').attributes('School Legal Name');\n    var schoolAddress=K('space').attributes('School Address');\n    info=info.replaceAll(\"$legalName$\", schoolLegalName);\n    info=info.replaceAll(\"$school_legal_name$\", schoolLegalName);    \n    info=info.replaceAll(\"$acn$\", acn);\n    info=info.replaceAll(\"$schoolName$\", schoolName);\n    info=info.replaceAll(\"$studentName$\", bridgedData['First Name']+\" \"+bridgedData['Last Name']);\n    info=info.replaceAll(\"$address$\", schoolAddress);\n    \n    $(\"#waiverInformation\").html(info);\n    \n    K('field[Waiver Information Text]').value($(\"#waiverInformation\").html());\n    \n    K('section[Waiver Section]').show();\n    K('section[Member Select Space]').hide();    \n }\n});\n\n",
